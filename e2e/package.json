{
  "name": "e2e",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm:watch:assets\" \"npm:serve\"",
    "start:ci": "npm run build && npm run serve",
    "serve": "eleventy --serve",
    "watch:assets": "npm run build:assets -- --watch",
    "build:assets": "microbundle -i src/assets/index.js  -o dist/index.js --no-pkg-main --external none",
    "build": "npm run build:assets && eleventy",
    "test": "start-server-and-test start:ci '8080' \"cypress run\"",
    "cypress:open": "cypress open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@testing-library/cypress": "^7.0.3",
    "concurrently": "^5.3.0",
    "microbundle": "^0.13.0",
    "postcss-import": "^14.0.0",
    "postcss-preset-env": "^6.7.0"
  },
  "dependencies": {
    "@10up/component-accordion": "^2.0.0"
  },
  "@10up/scripts": {
    "entry": {
      "index": "./src/assets/index.js",
      "style": "./src/assets/style.css"
    },
    "paths": {
      "cssLoaderPaths": [
        "./src/assets"
      ]
    }
  }
}
